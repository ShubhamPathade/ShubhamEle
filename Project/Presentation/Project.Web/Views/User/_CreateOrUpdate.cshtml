@model UserModel



<div class="row">
    <div class="col form-group">
        <input asp-for="Email" class="form-control" placeholder="Email" />
    </div>
</div>
<div class="row">
    <div class="col form-group">
        <input asp-for="MobileNo" class="form-control" placeholder="MobileNo" />
    </div>
</div>
<div class="row">
    <div class="col form-group">
        <input asp-for="Password"  class="form-control" placeholder="Password" />
    </div>
</div>
<div class="row">
    <div class="col form-group">
        <input asp-for="UserName"  class="form-control" placeholder="UserName" />
    </div>
</div>
<div class="row">
    <div class="col form-group">
        <select asp-for="SelectedRole" class="form-control" asp-items="Model.AvailableRoles">
            <option>Select Role</option>
        </select>
    </div>
</div>